{
	"name": "StorageEmpFileActual",
	"properties": {
		"linkedServiceName": {
			"referenceName": "devBlobStorage",
			"type": "LinkedServiceReference"
		},
		"parameters": {
			"FileName": {
				"type": "string",
				"defaultValue": "emp1.csv"
			}
		},
		"annotations": [],
		"type": "DelimitedText",
		"typeProperties": {
			"location": {
				"type": "AzureBlobStorageLocation",
				"fileName": {
					"value": "@dataset().FileName",
					"type": "Expression"
				},
				"folderPath": "emp",
				"container": "data"
			},
			"columnDelimiter": "|",
			"escapeChar": "\\",
			"firstRowAsHeader": true,
			"quoteChar": "\""
		},
		"schema": [
			{
				"name": "id",
				"type": "String"
			},
			{
				"name": "name",
				"type": "String"
			},
			{
				"name": "city",
				"type": "String"
			}
		]
	}
}